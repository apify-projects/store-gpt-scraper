    "devDependencies": {
        "@apify/eslint-config-ts": "^0.2.3",
        "@apify/tsconfig": "^0.1.0",
        "@jest/globals": "^29.5.0",
        "@types/async-retry": "^1.4.5",
        "@types/html-to-text": "^9.0.0",
        "@types/jest": "^29.5.0",
        "@types/turndown": "^5.0.1",
        "@typescript-eslint/eslint-plugin": "^5.32.0",
        "@typescript-eslint/parser": "^5.32.0",
        "eslint": "^8.20.0",
        "jest": "^29.5.0",
        "ts-jest": "^29.1.0",
        "ts-node": "^10.9.1",
        "typescript": "^4.7.4"
