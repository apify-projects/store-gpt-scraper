{
    "name": "@actors/extended-gpt-scraper",
    "description": "Example TypeScript actor in a monorepo",
    "version": "1.0.0",
    "type": "module",
    "private": true,
    "scripts": {
        "start": "node --no-warnings --experimental-specifier-resolution=node --loader ../../module_loader.js src/main.ts",
